@page
@model Artikeys_websiteonline_101022_mohmmedjar.Pages.customor.IndexModel
@{
}

<br>
<h2>List of Customor</h2>
<a class='btn btn-primary btn-sm' href='/Customor/Create'>New Customor</a>
<table calss="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Address</th>
            <th>Created_at</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.listcustomor)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>@item.email</td>
                <td>@item.phone</td>
                <td>@item.address</td>
                <td>@item.created_at</td>
                <td>
                    <a class="btn btn-primary btn-sm" href="/Customor/Edit?id=@item.id">Edit</a>
                    <a class="btn btn-danger btn-sm" href="/Customor/Delete?id=@item.id">Delete</a>
                </td>
            </tr>
        }
    </tbody>

</table>
